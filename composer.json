{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": ["framework", "laravel"],
  "license": "MIT",
  "require": {
    "php": "*",
    "bepsvpt/secure-headers": "^7.4",
    "blade-ui-kit/blade-heroicons": "^2.2.1",
    "doctrine/dbal": "^3.8",
    "fruitcake/laravel-cors": "^3.0",
    "genealabs/laravel-caffeine": "^9.0.4",
    "guzzlehttp/guzzle": "^7.8.1",
    "itsgoingd/clockwork": "^5.1.12",
    "laravel/framework": "^9.52.16",
    "laravel/jetstream": "^3.0.2",
    "laravel/sanctum": "^3.3.3",
    "laravel/tinker": "^2.9",
    "livewire/livewire": "^2.12.6",
    "mailchimp/marketing": "^3.0.80",
    "renatomarinho/laravel-page-speed": "^2.1",
    "spatie/laravel-webhook-client": "^3.2"
  },
  "require-dev": {
    "fakerphp/faker": "^1.23.1",
    "laravel/sail": "^1.27.2",
    "mockery/mockery": "^1.6.7",
    "nunomaduro/collision": "^6.4",
    "pestphp/pest": "^1.23.1",
    "pestphp/pest-plugin-laravel": "^1.4",
    "pestphp/pest-plugin-mock": "^1.0.3",
    "spatie/laravel-ignition": "^1.6.4"
  },
  "autoload": {
    "files": ["app/Helpers/ViewHelpers.php"],
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": ["@php artisan key:generate --ansi"],
    "test": ["pest"]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
